{{!
    @template block_test_block/main

    This template renders the main content area for the test block.

}}

<div id="block-test_block-{{uniqid}}" class="block-test_block block-cards" data-region="test_block" role="navigation">
    <div data-region="filter" class="d-flex align-items-center flex-wrap" aria-label="{{#str}} aria:controls, block_test_block {{/str}}">
        <ul>
        {{#modules}}
            <li>{{cmid}} - {{actname}} - {{cdate}} {{status}}</li>
        {{/modules}}
        </ul>
    </div>
</div>
